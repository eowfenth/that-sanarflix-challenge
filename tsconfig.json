{
	"compilerOptions": {
		"allowJs": true,
		"allowSyntheticDefaultImports": true,
		"esModuleInterop": true,
		"jsx": "react-native",
		"lib": ["es6"],
		"moduleResolution": "node",
		"noEmit": true,
		"strict": true,
		"target": "esnext",
		"alwaysStrict": true,
		"skipLibCheck": true
	}
